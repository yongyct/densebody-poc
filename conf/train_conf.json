        "epoch_count":1,
        "n_iter":100,
        "n_iter_decay":100,
        "save_result_freq":500,
        "save_epoch_freq":5,
        "beta1":0.5,
        "lr":0.0001,
        "lr_policy":"plateau",
        "lr_decay_iters":100,
        "tv_weight":10
    }

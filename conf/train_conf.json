{
    "job_conf":{
        "name":"densebody_resnet",
        "data_root":"mypath",
        "checkpoints_dir":"mypath",
        "dataset":"surreal",
        "max_dataset_size":-1,
        "im_size":256,
        "batch_size":10,
        "uv_map":"radvani",
        "num_threads":2
    },
    "model_conf":{
        "model":"resnet",
        "net_d":"convres",
        "nz":256,
        "ndown":6,
